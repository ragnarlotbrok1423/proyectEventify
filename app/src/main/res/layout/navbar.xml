<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/back_nav"
    android:layout_width="match_parent"
    android:layout_height="67dp"
    android:layout_alignParentLeft="true"
    android:layout_alignParentTop="true"
    android:layout_marginTop="777dp"
    android:background="@drawable/back_nav">

    <RelativeLayout
        android:id="@+id/navbar"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true">

        <ImageView
            android:id="@+id/button_home"
            android:layout_width="65dp"
            android:layout_height="60dp"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="25dp"
            android:layout_centerVertical="true"
            android:background="@drawable/home_button"
            android:contentDescription="Home Button" />

        <ImageView
            android:id="@+id/icofac"
            android:layout_width="52dp"
            android:layout_height="52dp"
            android:layout_toRightOf="@id/button_home"
            android:layout_marginLeft="35dp"
            android:layout_centerVertical="true"
            android:background="@drawable/fac_button"
            android:contentDescription="Fac Button" />

        <ImageView
            android:id="@+id/more_button"
            android:layout_width="52dp"
            android:layout_height="52dp"
            android:layout_toRightOf="@id/icofac"
            android:layout_marginLeft="41dp"
            android:layout_centerVertical="true"
            android:background="@drawable/more_button"
            android:contentDescription="More Button" />

        <ImageView
            android:id="@+id/person_butt"
            android:layout_width="52dp"
            android:layout_height="52dp"
            android:layout_toRightOf="@id/more_button"
            android:layout_marginLeft="39dp"
            android:layout_centerVertical="true"
            android:background="@drawable/people_button"
            android:contentDescription="People Button" />

    </RelativeLayout>
</RelativeLayout>
