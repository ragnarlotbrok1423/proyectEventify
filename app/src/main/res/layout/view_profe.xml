<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/perfil_de_p"
    android:layout_width="390dp"
    android:layout_height="947dp"
    android:clipToOutline="true"
    android:background="#FFFFFF">

    <include
        layout="@layout/navbar_profile" />

    <include
        layout="@layout/profile_comp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="204dp"
        android:layout_marginTop="91dp" />

    <RelativeLayout
        android:id="@+id/person"
        android:layout_width="109dp"
        android:layout_height="84dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="34dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="137dp">

        <RelativeLayout
            android:id="@+id/frame_26104"
            android:layout_width="115dp"
            android:layout_height="31dp">

            <TextView
                android:id="@+id/helena"
                android:layout_width="54dp"
                android:layout_height="24dp"
                android:text="@string/helena"
                android:textAppearance="@style/helena"
                android:lineSpacingExtra="5sp"
                android:translationY="-2.32sp"
                android:translationX="20sp"
                android:gravity="top"/>

            <TextView
                android:id="@+id/some_id"
                android:layout_width="14dp"
                android:layout_height="18dp"
                android:text="@string/some_id"
                android:textAppearance="@style/some_id"
                android:lineSpacingExtra="3sp"
                android:translationY="-1.74sp"
                android:gravity="top"/>
        </RelativeLayout>

        <TextView
            android:id="@+id/faculta_de_"
            android:layout_width="126dp"
            android:layout_height="42dp"
            android:text="@string/faculta_de_"
            android:textAppearance="@style/faculta_de_"
            android:lineSpacingExtra="3sp"
            android:translationY="30sp"
            android:gravity="top"/>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/button"
        android:layout_width="85dp"
        android:layout_height="34dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="296dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="226dp"
        android:clipToOutline="true"
        android:background="@drawable/button">

        <RelativeLayout
            android:id="@+id/state_layer"
            android:layout_width="85dp"
            android:layout_height="34dp">

            <TextView
                android:id="@+id/label_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:text="@string/seguir"
                android:textAppearance="@style/seguir"
                android:lineSpacingExtra="4sp"
                android:gravity="center" />

        </RelativeLayout>

    </RelativeLayout>
    <include
        layout="@layout/bar_comun"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="260dp"
        android:layout_marginLeft="10dp"/>
    <include layout="@layout/component_calificar_prof"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="370dp"/>
    <include layout="@layout/card_photo_list"
        android:layout_width="900dp"
        android:layout_height="900dp"
        android:background="@drawable/state_layer_background"
        android:layout_marginTop="480dp"/>


</RelativeLayout>
